extends layout_user

block breadcrumb

block content
  
  h4 I miei corsi
  
  ul
    for report in reports
      if report != null
        li
          a(href='/player/#{report.course.url}') #{report.course.name} 
          | &ndash; iscritto #{moment(report.events[report.events.length - 1].created_on).format('LLL')}
    
